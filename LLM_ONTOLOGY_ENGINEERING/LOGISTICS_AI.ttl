#################################################################
# Key OWL constraints (structural)
#################################################################

:Vehicle rdfs:subClassOf [
  a owl:Restriction ;
  owl:onProperty :ASSIGNED_TO ;
  owl:maxCardinality "1"^^xsd:nonNegativeInteger
] .

:origin_node a owl:ObjectProperty ; rdfs:domain :Route ; rdfs:range :Location .
:destination_node a owl:ObjectProperty ; rdfs:domain :Route ; rdfs:range :Location .

:Mission rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty :ORIGINATES_FROM ; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ; owl:onClass :Location ],
  [ a owl:Restriction ; owl:onProperty :DELIVERS_TO ; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ; owl:onClass :Location ] .

:Route rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty :origin_node ; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ; owl:onClass :Location ],
  [ a owl:Restriction ; owl:onProperty :destination_node ; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ; owl:onClass :Location ] .

