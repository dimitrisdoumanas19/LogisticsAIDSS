#################################################################
# Ontology header + lightweight modularization
#################################################################

:BattlefieldLogisticsOntology a owl:Ontology ;
  rdfs:label "Battlefield Logistics Decision Support Ontology"@en ;
  rdfs:comment "Modules: Spatial/Geographic, Resources/Supply, Missions/Operations, Organization, Events/Disruptions. OWL 2 DL."@en .

:ModuleSpatial a owl:Class ; rdfs:label "Spatial & Geographic Module"@en .
:ModuleResources a owl:Class ; rdfs:label "Resource & Supply Module"@en .
:ModuleMissions a owl:Class ; rdfs:label "Mission & Operations Module"@en .
:ModuleOrganization a owl:Class ; rdfs:label "Organizational Module"@en .
:ModuleEvents a owl:Class ; rdfs:label "Events & Disruptions Module"@en .

#################################################################
# Enumerations (OWL 2 oneOf)
#################################################################

:OperationalStatus a owl:Class ; rdfs:label "Operational Status"@en ;
  owl:oneOf ( :OPERATIONAL :DEGRADED :DESTROYED :NON_OPERATIONAL ) .

:RouteStatus a owl:Class ; rdfs:label "Route Status"@en ;
  owl:oneOf ( :ROUTE_OPERATIONAL :ROUTE_DEGRADED :ROUTE_DAMAGED :ROUTE_BLOCKED ) .

:ThreatLevel a owl:Class ; rdfs:label "Threat Level"@en ;
  owl:oneOf ( :THREAT_LOW :THREAT_MEDIUM :THREAT_HIGH :THREAT_CRITICAL ) .

:WeatherImpact a owl:Class ; rdfs:label "Weather Impact"@en ;
  owl:oneOf ( :WEATHER_MINOR :WEATHER_MODERATE :WEATHER_SEVERE ) .

:ProtectionLevel a owl:Class ; rdfs:label "Protection Level"@en ;
  owl:oneOf ( :PROT_HIGH :PROT_MEDIUM :PROT_LOW ) .

:DependencyLevel a owl:Class ; rdfs:label "Dependency Level"@en ;
  owl:oneOf ( :DEP_CRITICAL :DEP_HIGH :DEP_MEDIUM :DEP_LOW ) .

:PriorityLevel a owl:Class ; rdfs:label "Priority"@en ;
  owl:oneOf ( :PRIO_CRITICAL :PRIO_HIGH :PRIO_MEDIUM :PRIO_LOW ) .

:CriticalityLevel a owl:Class ; rdfs:label "Resource Criticality"@en ;
  owl:oneOf ( :CRIT_CRITICAL :CRIT_HIGH :CRIT_MEDIUM :CRIT_LOW ) .

:ResourceDependency a owl:Class ; rdfs:label "Resource Dependency"@en ;
  owl:oneOf ( :DEP_NONE :DEP_LOW2 :DEP_MEDIUM2 :DEP_HIGH2 ) .

:VehicleStatus a owl:Class ; rdfs:label "Vehicle Status"@en ;
  owl:oneOf ( :VEH_OPERATIONAL :VEH_MAINTENANCE :VEH_DESTROYED ) .

:ArmorLevel a owl:Class ; rdfs:label "Armor Level"@en ;
  owl:oneOf ( :ARMOR_NONE :ARMOR_LIGHT :ARMOR_MEDIUM :ARMOR_HEAVY ) .

:MissionStatus a owl:Class ; rdfs:label "Mission Status"@en ;
  owl:oneOf ( :M_PLANNED :M_IN_PROGRESS :M_COMPLETED :M_FAILED ) .

:VehicleMissionRole a owl:Class ; rdfs:label "Vehicle Mission Role"@en ;
  owl:oneOf ( :ROLE_PRIMARY_TRANSPORT :ROLE_ESCORT :ROLE_AIR_TRANSPORT ) .

:AssignmentType a owl:Class ; rdfs:label "Assignment Type"@en ;
  owl:oneOf ( :ASSIGN_OPERATIONAL :ASSIGN_HQ :ASSIGN_DESTROYED ) .

:RelationshipStrength a owl:Class ; rdfs:label "Relationship Strength"@en ;
  owl:oneOf ( :RS_NONE :RS_LOW :RS_MEDIUM :RS_HIGH :RS_CRITICAL ) .

:ImpactSeverity a owl:Class ; rdfs:label "Impact Severity"@en ;
  owl:oneOf ( :IMPACT_CRITICAL :IMPACT_HIGH :IMPACT_MEDIUM :IMPACT_LOW ) .

:UnitOfMeasure a owl:Class ; rdfs:label "Unit of Measure"@en ;
  owl:oneOf ( :UOM_LITERS :UOM_UNITS :UOM_RATIONS :UOM_KG :UOM_KM :UOM_HOURS ) .

