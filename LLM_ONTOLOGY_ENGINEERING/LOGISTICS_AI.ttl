#################################################################
# Core Classes
#################################################################

### Location hierarchy
:Location a owl:Class ; rdfs:label "Location"@en .
:MainBase a owl:Class ; rdfs:subClassOf :Location ; rdfs:label "Main Base"@en .
:ForwardDepot a owl:Class ; rdfs:subClassOf :Location ; rdfs:label "Forward Depot"@en .
:CombatPosition a owl:Class ; rdfs:subClassOf :Location ; rdfs:label "Combat Position"@en .

### Route
:Route a owl:Class ; rdfs:label "Route"@en .

### Military units
:MilitaryUnit a owl:Class ; rdfs:label "Military Unit"@en .
:LogisticsUnit a owl:Class ; rdfs:subClassOf :MilitaryUnit ; rdfs:label "Logistics Unit"@en .
:CombatUnit a owl:Class ; rdfs:subClassOf :MilitaryUnit ; rdfs:label "Combat Unit"@en .

:CommandLevel a owl:Class ; rdfs:label "Command Level"@en ;
  owl:oneOf ( :BrigadeLevel :BattalionLevel :CompanyLevel ) .
:BrigadeLevel a owl:NamedIndividual .
:BattalionLevel a owl:NamedIndividual .
:CompanyLevel a owl:NamedIndividual .

### Resources
:Resource a owl:Class ; rdfs:label "Resource"@en .
:Fuel a owl:Class ; rdfs:subClassOf :Resource ; rdfs:label "Fuel"@en .
:Ammunition a owl:Class ; rdfs:subClassOf :Resource ; rdfs:label "Ammunition"@en .
:Food a owl:Class ; rdfs:subClassOf :Resource ; rdfs:label "Food"@en .
:Water a owl:Class ; rdfs:subClassOf :Resource ; rdfs:label "Water"@en .
:Medical a owl:Class ; rdfs:subClassOf :Resource ; rdfs:label "Medical"@en .

### Vehicles
:Vehicle a owl:Class ; rdfs:label "Vehicle"@en .
:HeavyTruck a owl:Class ; rdfs:subClassOf :Vehicle ; rdfs:label "Heavy Truck"@en .
:LightVehicle a owl:Class ; rdfs:subClassOf :Vehicle ; rdfs:label "Light Vehicle"@en .
:AircraftTransport a owl:Class ; rdfs:subClassOf :Vehicle ; rdfs:label "Aircraft Transport"@en .

### Missions
:Mission a owl:Class ; rdfs:label "Mission"@en .
:ResupplyMission a owl:Class ; rdfs:subClassOf :Mission ; rdfs:label "Resupply Mission"@en .
:EmergencyResupply a owl:Class ; rdfs:subClassOf :Mission ; rdfs:label "Emergency Resupply"@en .
:ReconnaissanceMission a owl:Class ; rdfs:subClassOf :Mission ; rdfs:label "Reconnaissance Mission"@en .
:AlternativeSupply a owl:Class ; rdfs:subClassOf :Mission ; rdfs:label "Alternative Supply"@en .

### Disruptions
:Disruption a owl:Class ; rdfs:label "Disruption"@en .
:FacilityDestruction a owl:Class ; rdfs:subClassOf :Disruption ; rdfs:label "Facility Destruction"@en .
:RouteDamage a owl:Class ; rdfs:subClassOf :Disruption ; rdfs:label "Route Damage"@en .
:InfrastructureDamage a owl:Class ; rdfs:subClassOf :Disruption ; rdfs:label "Infrastructure Damage"@en .
:WeatherImpactDisruption a owl:Class ; rdfs:subClassOf :Disruption ; rdfs:label "Weather Impact"@en .
:ResourceDepletion a owl:Class ; rdfs:subClassOf :Disruption ; rdfs:label "Resource Depletion"@en .

### Events
:Event a owl:Class ; rdfs:label "Event"@en .
:AttackEvent a owl:Class ; rdfs:subClassOf :Event ; rdfs:label "Attack Event"@en .
:InfrastructureDamageEvent a owl:Class ; rdfs:subClassOf :Event ; rdfs:label "Infrastructure Damage Event"@en .
:StructuralFailureEvent a owl:Class ; rdfs:subClassOf :Event ; rdfs:label "Structural Failure Event"@en .
:WeatherEvent a owl:Class ; rdfs:subClassOf :Event ; rdfs:label "Weather Event"@en .
:ResourceAlertEvent a owl:Class ; rdfs:subClassOf :Event ; rdfs:label "Resource Alert Event"@en .
:MissionEvent a owl:Class ; rdfs:subClassOf :Event ; rdfs:label "Mission Event"@en .
:MaintenanceEvent a owl:Class ; rdfs:subClassOf :Event ; rdfs:label "Maintenance Event"@en .
:CheckpointEvent a owl:Class ; rdfs:subClassOf :Event ; rdfs:label "Checkpoint Event"@en .

#################################################################
# Object Properties (relationships)
#################################################################

:CONNECTS_TO a owl:ObjectProperty ;
  rdfs:domain :Location ; rdfs:range :Location ;
  rdfs:label "connects to"@en .

:DEPENDS_ON a owl:ObjectProperty ;
  rdfs:domain :Location ; rdfs:range :Route ;
  rdfs:label "depends on"@en .

:ALTERNATIVE_TO a owl:ObjectProperty ;
  rdfs:domain :Route ; rdfs:range :Route ;
  rdfs:label "alternative to"@en .

:STORES a owl:ObjectProperty ;
  rdfs:domain :Location ; rdfs:range :Resource ;
  rdfs:label "stores"@en .

:SUPPLIES a owl:ObjectProperty ;
  rdfs:domain :Location ; rdfs:range :Location ;
  rdfs:label "supplies"@en .

:REQUIRES a owl:ObjectProperty ;
  rdfs:label "requires"@en .

:ASSIGNED_TO a owl:ObjectProperty ;
  rdfs:domain :Vehicle ; rdfs:range :Mission ;
  rdfs:label "assigned to"@en .

:ORIGINATES_FROM a owl:ObjectProperty ;
  rdfs:domain :Mission ; rdfs:range :Location ;
  rdfs:label "originates from"@en .

:DELIVERS_TO a owl:ObjectProperty ;
  rdfs:domain :Mission ; rdfs:range :Location ;
  rdfs:label "delivers to"@en .

:USES_ROUTE a owl:ObjectProperty ;
  rdfs:domain :Mission ; rdfs:range :Route ;
  rdfs:label "uses route"@en .

:LOCATED_AT a owl:ObjectProperty ;
  rdfs:label "located at"@en .

:AFFECTS a owl:ObjectProperty ;
  rdfs:domain :Disruption ; rdfs:label "affects"@en .

:CASCADES_TO a owl:ObjectProperty ;
  rdfs:domain :Disruption ; rdfs:range :Disruption ;
  rdfs:label "cascades to"@en .

:parent_unit a owl:ObjectProperty ;
  rdfs:domain :MilitaryUnit ; rdfs:range :MilitaryUnit ;
  rdfs:label "parent unit"@en .

:location_node a owl:ObjectProperty ;
  rdfs:domain :MilitaryUnit ; rdfs:range :Location ;
  rdfs:label "assigned location"@en .

